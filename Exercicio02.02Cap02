<!DOCTYPE html>

<html>
<head>
  <meta http-equiv="CONTENT-TYPE" content="text/html; charset=UTF-8">
  <link rel="stylesheet" href="styles/style.css"/>
  <title>Exercicio 02.02</title>
</head>
<body>
  <h1>
    Exercicio 02.02
  </h1>
  
  <script>
  
  for(let num = 1; num <= 100; num++){
      if(num % 3 == 0 && num % 5 == 0){
          document.write("FizzBuzz");
      }else if(num % 3 == 0){
          document.write("Fizz");
      }else if(num % 5 == 0){
          document.write("Buzz");
      }else{
          document.write(num);
      }
      document.write("<br>");
  }

  </script>

</body>
</html>

